{
  "name": "apek",
  "version": "1.0.0",
  "description": "Asosiasi Pengusaha Engineering Karawang",
  "author": "Eko Eryanto",
  "repository": {
    "type": "git",
    "url": "https://github.com/ekoeryanto/apek.git"
  },
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "build:admin": "node ./admin/build.js",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "lint-staged",
    "prepush": "npm run build:admin"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "prettier --ignore-path .gitignore --parser vue --single-quote --trailing-comma es5 --write",
      "eslint --ignore-path .gitignore --fix",
      "git add"
    ],
    "*.{png,jpeg,jpg,gif,svg}": [
      "node ./thumbnail.js",
      "git add",
      "imagemin-lint-staged",
      "git add"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.5",
    "@fortawesome/fontawesome-free-brands": "^5.0.10",
    "@fortawesome/fontawesome-free-regular": "^5.0.10",
    "@fortawesome/fontawesome-free-solid": "^5.0.10",
    "@fortawesome/react-fontawesome": "0.0.18",
    "@fortawesome/vue-fontawesome": "0.0.22",
    "@nuxtjs/google-analytics": "^2.0.2",
    "@nuxtjs/pwa": "^2.0.8",
    "@nuxtjs/sitemap": "0.0.5",
    "babel-eslint": "^8.2.1",
    "babel-plugin-transform-imports": "^1.5.0",
    "crawler-user-agents": "0.0.1",
    "cross-env": "^5.0.1",
    "eslint": "^4.15.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-vue": "^4.0.0",
    "gh-pages": "^1.1.0",
    "husky": "^0.14.3",
    "image-process-loader": "^0.1.1",
    "image-size": "^0.6.2",
    "imagemin-lint-staged": "^0.3.0",
    "lint-staged": "^7.0.4",
    "marked": "^0.3.19",
    "material-icons": "^0.2.1",
    "netlify-cms": "^1.5.0",
    "nuxt": "^1.4.0",
    "nuxt-fontawesome": "^0.1.3",
    "parcel-bundler": "^1.7.0",
    "prettier": "^1.12.0",
    "react-select": "^1.2.1",
    "sharp": "^0.20.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "vue-recaptcha": "^1.1.0",
    "vuetify": "^1.0.14"
  }
}
