{
  "name": "apek",
  "version": "1.0.0",
  "description": "Asosiasi Pengusaha Engineering Karawang",
  "author": "Eko Eryanto",
  "repository": {
    "type": "git",
    "url": "https://github.com/ekoeryanto/apek.git"
  },
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "prettier --ignore-path .gitignore --parser vue --single-quote --trailing-comma es5 --write",
      "eslint --ignore-path .gitignore --fix",
      "git add"
    ],
    "*.{png,jpeg,jpg,gif,svg}": [
      "imagemin-lint-staged",
      "git add"
    ]
  },
  "dependencies": {
    "@nuxtjs/markdownit": "^1.2.0",
    "@nuxtjs/markdownit-loader": "^1.1.1",
    "@nuxtjs/pwa": "^2.0.8",
    "nuxt": "^1.0.0",
    "quasar-extras": "0.0.8",
    "vue-material-design-icons": "^1.2.1",
    "vue-recaptcha": "^1.1.0",
    "vue-typer": "^1.2.0",
    "vuetify": "^1.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "babel-plugin-transform-imports": "^1.5.0",
    "cross-env": "^5.0.1",
    "eslint": "^4.15.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.0.0",
    "husky": "^0.14.3",
    "imagemin-lint-staged": "^0.3.0",
    "lint-staged": "^7.0.0",
    "prettier": "^1.11.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1"
  }
}
